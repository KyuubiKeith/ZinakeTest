                            <source src="~/Content/Images/AOB/video/telefono_def.mp4" type="video/mp4">
                        </video>
                    </div>
                    <div class="no-padding hiring-thumb last">
                        <div class="static-video-thumb hidden-lg" style="background-image:url('~/Content/Images/AOB/video/static-videos/6.jpg');"></div>
