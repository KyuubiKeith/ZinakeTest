    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.2.0/css/all.css" integrity="sha384-hWVjflwFxL6sNzntih27bfxkr27PmbbK/iSvJ+a4+0owXq79v+lsFkW54bOGbiDQ" crossorigin="anonymous">

</head>


<body>

    <!-- Background Video -->
    <video class="bg-video" loop autoplay muted>
        <source src="../Artista/Shared/Videos/Push-The-Buttons.mp4" type="video/mp4">
    </video>
    <!-- Background Video -->
    <!-- Mask -->
    <div class="mask"></div>
    <!-- Mask -->
    <!-- Navbar -->
    <nav class="navbar navbar-default navbar-fixed-top">
        <div class="logo-container hidden-sm hidden-xs"><div class="static-logo"></div><div class="logo-gif"></div></div>
        <div class="logo-container mobile visible-sm visible-xs"><div class="static-logo black"></div></div>
        <div class="social-show-container"><a class="social-open" href="#"><span class="hidden-xs">Register</span><i class="fas fa-arrow-right"></i></a></div>
    </nav>
    <!-- Navbar -->
    <!-- Dynamic Content -->
    <section class="wrapper">
        <div class="border-element"></div>
        <div class="landing scene" id="demo-slider">
            <div class="slides workinprogress active">

                <!-- Dynamic Content is rendered here -->
                <section class="visible-section"></section>
                <!-- Dynamic  Content is rendered here -->
                <!-- Static Content -->
                <div class="hidden-section">
                    <div class="col-center padding-bottom-mobile">

                        <h2 class="not-uppercase">
                            Welcome to <br class="hidden-sm hidden-md hidden-lg">
                            <div class="typed-string-container">
                                <div class="profile-pic second"></div>
                                <span class="second-typed-string"> Our Online Platform</span>
                            </div><br class="hidden-md hidden-lg">.
                        </h2>

                        <div class="text-primary">
                            Use This Portal to
                            <div class="typed-string-container">
                                <span class="first-typed-string">Register</span>
                                <div class="profile-pic first"></div>
                            </div>.<br><br>
                            <div class="typed-string-container">
                                <span class="third-typed-string"></span>
                                <div class="profile-pic third"></div>
                            </div>
                        </div>

                    </div>
                </div>
                <!-- Static Content -->

            </div>
        </div>
    </section>
    <!-- Dynamic Content -->

    <!-- Pop-Up -->
    <div class="social-popup">
        <div class="velina"></div>
        <div class="social-close"><a class="social-close-button" href="#"><i class="fas fa-times"></i></a></div>
        <div class="social-container">
            <h2 class="white">You can find us on:</h2>
            <div class="social-buttons-content">
                <div class="social-button-container">
                    <a class="social-links fb hover-state" target="_blank" href="https://www.facebook.com/fightbean"><div class="icon-container"><i class="icon icon-fb"></i></div></a>
                    <a class="social-links fb normal" target="_blank" href="https://www.facebook.com/fightbean"><div class="icon-container"><i class="icon icon-fb"></i></div></a>
                </div>
                <div class="social-button-container">
                    <a class="social-links dribbble hover-state" target="_blank" href="https://dribbble.com/fightbean"><div class="icon-container"><i class="icon icon-dribbble"></i></div></a>
                    <a class="social-links dribbble normal" target="_blank" href="https://dribbble.com/fightbean"><div class="icon-container"><i class="icon icon-dribbble"></i></div></a>
                </div>
                <div class="social-button-container">
                    <a class="social-links instagram hover-state" target="_blank" href="https://www.instagram.com/fightbean/"><div class="icon-container"><i class="icon icon-instagram"></i></div></a>
                    <a class="social-links instagram normal" target="_blank" href="https://www.instagram.com/fightbean/"><div class="icon-container"><i class="icon icon-instagram"></i></div></a>
                </div>
                <div class="social-button-container">
                    <a class="social-links be hover-state" target="_blank" href="https://www.behance.net/fightbean"><div class="icon-container"><i class="icon icon-be"></i></div></a>
                    <a class="social-links be normal" target="_blank" href="https://www.behance.net/fightbean"><div class="icon-container"><i class="icon icon-be"></i></div></a>
                </div>
            </div>

        </div>
    </div>
    <!-- Pop-Up -->
    </div>

    <!-- Scripts -->
    <script src="../Artista/Landing/The_Styles/JavaScript/cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js"></script>
