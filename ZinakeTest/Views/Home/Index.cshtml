                        </li>
                        <li>
                            <img class="img-responsive" src="~/Content/Images/AOB/video/static-videos/6.jpg" />
                        </li>
                    </ul>
                </div>
            </div>
            <!-- Ads Section-->
            <!-- Page 3 Content -->
            <div class="slides sview-section">
                <div class="left-layer-show">
                    <div class="arrow-section-container"><a class="street-view-toggle" href="#"><i class="icon icon-arrow-down"></i></a></div>
                </div>
