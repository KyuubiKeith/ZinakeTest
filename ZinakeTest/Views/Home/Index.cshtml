    <!-- Favicon -->
    <link rel="icon" type="image/svg" href="../Artista/Shared/Images/teaching.svg" />
    <link rel="icon" type="image/png" href="../Artista/Shared/Images/teaching.png" />

    <!-- mobile setting -->
