@section PopUps
{
    <!-- Pop-Up Social -->
    <div class="social-popup">
        <div class="velina"></div>
        <div class="social-close"><a class="social-close-button" href="#"><i class="fas fa-times"></i></a></div>
        <div class="social-container">
            <h2 class="white">You can find us on:</h2>
            <div class="social-buttons-content">
                <div class="social-button-container">
                    <a class="social-links fb hover-state" target="_blank" href="https://www.facebook.com/fightbean"><div class="icon-container"><i class="fab fa-twitter"></i></div></a>
                    <a class="social-links fb normal" target="_blank" href="https://www.facebook.com/fightbean"><div class="icon-container"><i class="fab fa-twitter"></i></div></a>
                </div>
                <div class="social-button-container">
                    <a class="social-links dribbble hover-state" target="_blank" href="https://dribbble.com/fightbean"><div class="icon-container"><i class="fab fa-dribbble"></i></div></a>
                    <a class="social-links dribbble normal" target="_blank" href="https://dribbble.com/fightbean"><div class="icon-container"><i class="fab fa-dribbble"></i></div></a>
                </div>
                <div class="social-button-container">
                    <a class="social-links instagram hover-state" target="_blank" href="https://www.instagram.com/fightbean/"><div class="icon-container"><i class="fab fa-instagram"></i></div></a>
                    <a class="social-links instagram normal" target="_blank" href="https://www.instagram.com/fightbean/"><div class="icon-container"><i class="fab fa-instagram"></i></div></a>
                </div>
                <div class="social-button-container">
                    <a class="social-links be hover-state" target="_blank" href="https://www.behance.net/fightbean"><div class="icon-container"><i class="fab fa-behance"></i></div></a>
                    <a class="social-links be normal" target="_blank" href="https://www.behance.net/fightbean"><div class="icon-container"><i class="fab fa-behance"></i></div></a>
                </div>
            </div>

        </div>
    </div>
    <!-- Pop-Up Social -->
    <!-- Pop-Up Jobs-->
    <div class="popup-outer" id="jobs">
        <div class="velina"></div>
        <div class="popup-inner">
            <div class="mobile-header-solid">
                <div class="mob-close-button-container visible-sm visible-xs"><a href="#" class="mob-close"><i class="icon icon-close"></i></a></div>
            </div>
            <a href="#" class="close-button-container close-button hidden-sm hidden-xs">
                <div class="rotated-button">
                    <span class="close-button" href="#">CLOSE</span>
                </div>
            </a>
            <div class="overlay-content">
                <div class="popup-content padding-big">
                    <div class="div-700-max">
                        <header class="popup-header">
                            <h1 class="medium-title">Being a bean</h1>
                            <p>We are always on the look out for creative makers and thinkers across project management, ux, ui,  visual design and development. <br><b>Join our great team!</b></p>
                        </header>
                    </div>
                </div>
                <div class="popup-content padding-big">
                    <div class="popup-main-content">
                        <div class="jobs-listing">
                            <div class="div-700-max">
                                <h5 class="jobs-subtitle"><b>There are no current openings.</b></h5>
                                <p>Please apply to <a href="mailto:jobs@fightbean.it">jobs@fightbean.it</a> and we will consider you for future positions.</p>
                            </div>
                            <!--
                                                            <div class="div-700-max">
                                                                <h5 class="jobs-subtitle">We are looking for</h5>
                                                                <ul class="jobs-list">
                                                                    <li><a href="/jobs/ux-designer.html">1. Ux designer</a></li>
                                                                </ul>
                                                            </div>
                            -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Pop-Up Jobs-->
    <!-- Pop-Up Contacts -->
    <div class="popup-outer" id="contact-us">
        <div class="velina"></div>
        <div class="popup-inner">
            <div class="mobile-header-solid">
                <div class="mob-close-button-container visible-sm visible-xs"><a href="#" class="mob-close"><i class="icon icon-close"></i></a></div>
            </div>
            <a href="#" class="close-button-container close-button hidden-sm hidden-xs">
                <div class="rotated-button">
                    <span class="close-button" href="#">CLOSE</span>
                </div>
            </a>
            <div class="overlay-content">
                <div class="popup-content padding-big">
                    <div class="div-700-max">
                        <header class="popup-header">
                            <h1 class="medium-title">Let's keep in touch </h1>
                            <p>We are in office monday-friday, 9-18. <br>Drop us a line here, send us an email or call.</p>
                            <form name="contactForm" class="input-styled" id="formDefault">
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <input type="text" name="name" id="contactName" class="form-control">
                                            <label for="contactName">Name</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <input type="number" name="telephone" id="contactPhone" class="form-control">
                                            <label for="contactPhone">Telephone</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <input type="email" name="email" id="contactEmail" class="form-control required">
                                            <label for="contactEmail">Email address</label>
                                            <span class="error-text empty-text">Questo campo è obbligatorio!</span>
                                            <span class="error-text mail">La mail non è valida!</span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group last">
                                            <textarea name="text" id="textMessage" class="form-control"></textarea>
                                            <label for="textMessage">Your message</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="normal-button-container">
                                    <a data-target="#follow" href="#" class="btn btn-default normal-links hover-state submit">Send<div class="icon-container"><i class="icon icon-arrow-right"></i></div></a>
                                    <a data-target="#follow" href="#" class="btn btn-default normal-links normal submit">Send<div class="icon-container"><i class="icon icon-arrow-right"></i></div></a>
                                </div>
                            </form>
                            <div class="success-message">
                                <h2 class="green">THANK YOU <span class="name-value">placeholder</span>!</h2>
                                <p>
                                    We will get back to you soon. <br>Good design never sleep. But we do. Sometimes.<br>
                                    Do you want to <a class="mostra-form" href="#">send another message?</a>
                                </p>
                            </div>
                        </header>
                    </div>
                </div>
                <div class="popup-content padding-big">
                    <h1 class="medium-title green">Come and stop in, we have coffee.</h1>
                    <div class="div-700-max">
                        <div class="row address-blocks-container">
                            <div class="col-md-6 address-blocks">
                                <div class="address-block-content">
                                    <h6>HEADQUARTER</h6>
                                    <p>via Perrone 5<br>10125 Torino, ITALY<br>Tel. + 39 011 19 47 83 11 / 011 19 47 83 10<br>Email <a href="mailto:hello@fightbean.it">hello@fightbean.it</a></p>
                                </div>
                            </div>
                            <div class="col-md-6 address-blocks clearfix">
                                <div class="address-block-content">
                                    <h6>NEW BUSINESS</h6>
                                    <p>Federica Pecoraro<br>Tel. +39 347 77 28 264<br>Email <a href="mailto:federica.pecoraro@fightbean.it">federica.pecoraro@fightbean.it</a><br>Matteo Botto<br>Tel. +39 333 59 95 248<br>Email <a href="mailto:matteo.botto@fightbean.it">matteo.botto@fightbean.it</a></p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="popup-content padding-small">
                    <div id="map"></div>
                </div>
            </div>
        </div>
    </div>
    <!-- Pop-Up Contacts -->

}
