    <script src="../Artista/Landing/The_Styles/JavaScript/res/velocity.ui.min.js"></script>
    <script src="../Artista/Landing/The_Styles/JavaScript/vertical-slider.js"></script>
    <script src="../Artista/Landing/The_Styles/JavaScript/lightslider.js"></script>
    <script src="../Artista/Landing/The_Styles/JavaScript/main.js"></script>
